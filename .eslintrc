{
	"env": {
        "browser": true,
		"node": true,
		"mocha": true,
		"jest": true
    },
	"extends":  [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"modules": true
		}
	},
	"plugins": [
		"react"
	],
	"rules": {
		"no-console": "off"
	}
}